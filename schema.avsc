{
  "type": "record",
  "doc": "Schema for magstats step, with detections and non detections",
  "name": "magstats",
  "fields": [
    {
      "name": "aid",
      "type": "string"
    },
    {
      "name": "detections",
      "type": {
        "type": "array",
        "items": {
        "type": "record",
        "name": "alert",
        "fields": [
          {
            "name": "oid",
            "type": "string"
          },
          {
            "name": "tid",
            "type": "string"
          },
          {
            "name": "pid",
            "type": "long"
          },
          {
            "name": "candid",
            "type": ["long", "string"]
          },
          {
            "name": "mjd",
            "type": "double"
          },
          {
            "name": "fid",
            "type": "int"
          },
          {
            "name": "ra",
            "type": "double"
          },
          {
            "name": "dec",
            "type": "double"
          },
          {
            "name": "mag",
            "type": "float"
          },
          {
            "name": "e_mag",
            "type": "float"
          },
          {
            "name": "isdiffpos",
            "type": "int"
          },
          {
            "name": "e_ra",
            "type": "float"
          },
          {
            "name": "e_dec",
            "type": "float"
          },
          {
            "name": "extra_fields",
            "type": {
              "default": {},
              "type": "map",
              "values": ["null", "int", "float", "string", "bytes", "boolean"]
            }
          },
          {
            "name": "aid",
            "type": "string"
          },
          {
            "name": "corrected",
            "type": "boolean"
          },
          {
            "name": "dubious",
            "type": "boolean"
          },
          {
            "name": "has_stamp",
            "type": "boolean"
          },
          {
            "name": "mag_corr",
            "type": ["float", "null"]
          },
          {
            "name": "e_mag_corr",
            "type": ["float", "null"]
          },
          {
            "name": "e_mag_corr_ext",
            "type": ["float", "null"]
          }
        ]
      },
        "default": []
      }
    },
    {
      "name": "non_detections",
      "type": {
        "type": "array",
        "items": {
          "type": "record",
          "name": "non_detection",
          "fields": [
            {
              "name": "aid",
              "type": "string"
            },
            {
              "name": "tid",
              "type": "string"
            },
            {
              "name": "oid",
              "type": "string"
            },
            {
              "name": "mjd",
              "type": "double"
            },
            {
              "name": "fid",
              "type": "int"
            },
            {
              "name": "diffmaglim",
              "type": "int"
            }
          ]
        }
      },
      "default": []
    }
  ]
}
